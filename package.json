{
	"name": "vite-plugin-hono-file-route-generator",
	"version": "0.0.3",
	"description": "A vite plugin that scans a folder, grabs all default exports, and generates a js/ts file that exports an array for mounting.",
	"main": "./dist/index.cjs",
	"module": "./dist/index.mjs",
	"types": "./dist/index.d.cts",
	"exports": {
		"require": {
			"types": "./dist/index.d.cts",
			"default": "./dist/index.cjs"
		},
		"import": {
			"types": "./dist/index.d.mts",
			"default": "./dist/index.mjs"
		}
	},
	"private": false,
	"scripts": {
		"dev": "pkgroll --watch",
		"build": "pkgroll --minify"
	},
	"keywords": [
		"hono",
		"file",
		"router",
		"vite",
		"plugin"
	],
	"author": "thisjt",
	"license": "MIT",
	"type": "module",
	"devDependencies": {
		"@eslint/js": "^9.12.0",
		"@types/eslint__js": "^8.42.3",
		"@types/node": "^22.7.5",
		"eslint": "^9.12.0",
		"eslint-config-prettier": "^9.1.0",
		"globals": "^15.11.0",
		"pkgroll": "^2.5.0",
		"prettier": "^3.3.3",
		"typescript": "^5.6.3",
		"typescript-eslint": "^8.9.0"
	}
}
